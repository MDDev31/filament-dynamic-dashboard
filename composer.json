{
    "name": "mddev31/filament-dynamic-dashboard",
    "description": "Dynamic dashboard for Laravel Filament.",
    "type": "library",
    "keywords": [
        "laravel",
        "filament",
        "dashboard"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mddev31/filament-dynamic-dashboard",
    "support": {
        "issues": "https://github.com/mddev31/filament-dynamic-dashboard/issues",
        "source": "https://github.com/mddev31/filament-dynamic-dashboard"
    },
    "authors": [
        {
            "name": "Maxime DANJOU",
            "email": "maxime@mddev.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.0|^5.0",
        "spatie/laravel-package-tools": "^1.92",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^4.0",
        "phpunit/phpunit": "^12.5",
        "spatie/pest-plugin-test-time": "^2.2",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MDDev\\DynamicDashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MDDev\\DynamicDashboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MDDev\\DynamicDashboard\\FilamentDynamicDashboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
